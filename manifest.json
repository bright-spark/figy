{
  "name": "AI UI Converter",
  "id": "ai-ui-converter",
  "api": "1.0.0",
  "main": "dist/code.js",
  "ui": "dist/ui.html",
  "editorType": ["figma", "dev"],
  "networkAccess": {
    "allowedDomains": ["https://openai.com", "https://api.openai.com"],
    "reasoning": "Required for OpenAI API access to analyze images and generate UI elements"
  },
  "permissions": [
    "currentuser",
    "activeusers"
  ],
  "capabilities": [
    "inspect",
    "codegen",
    "textreview",
    "vscode"
  ],
  "codegenLanguages": [
    {
      "label": "TypeScript",
      "value": "typescript"
    },
    {
      "label": "JavaScript",
      "value": "javascript"
    },
    {
      "label": "React",
      "value": "react"
    }
  ],
  "relaunchButtons": [
    {
      "command": "analyze",
      "name": "Analyze Image",
      "multipleSelection": false
    }
  ]
}
